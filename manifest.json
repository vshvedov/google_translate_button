{
  "name": "A Google Tanslate Button",
  "description": "A Google Translate in a popup. Without new tabs or windows. Right on place.",
  "version": "0.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_icon": "icon.png",
      "popup": "popup.html"
  },
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" }
}
